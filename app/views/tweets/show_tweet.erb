<h2><%= @tweet.content %></h2>


<form action="/tweets/<%= @tweet.id %>/edit">
  <button type="submit">Edit Tweet</button>
</form>
<form method="post" action="/tweets/<%= @tweet.id %>">
  <input id="hidden" type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Delete Tweet">
</form>
<form action="/tweets">
  <button type="submit">Tweets</button>
</form>
