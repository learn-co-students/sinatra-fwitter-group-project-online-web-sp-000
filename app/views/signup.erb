<h2>Sign Up Here</h2>

  <form action="/signup" method="post">
    <p>
      <label>Username:</label>
      <input type="text" name="username">
    </p>
    <p>
      <label>Email:</label>
      <input type="text" name="email">
    </p>
    <p>
      <label>Password:</label>
      <input type="password" name="password">
    </p>
    <input type="submit" name="Submit">
  </form>
